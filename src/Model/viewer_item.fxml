<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="viewerHBox" alignment="TOP_CENTER" prefHeight="27.0" prefWidth="648.0" styleClass="light-gray-background" stylesheets="@../values/styles.css" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="main.ViewerItemController">
   <children>
      <Label text="Viewer:">
         <font>
            <Font name="Consolas" size="15.0" />
         </font>
         <HBox.margin>
            <Insets left="5.0" top="4.0" />
         </HBox.margin>
      </Label>
      <TextField fx:id="usernameField" prefHeight="25.0" prefWidth="172.0" promptText="Username">
         <HBox.margin>
            <Insets top="1.0" />
         </HBox.margin></TextField>
      <TextField fx:id="pathField" prefHeight="25.0" prefWidth="285.0" promptText="Path to File">
         <HBox.margin>
            <Insets left="50.0" top="1.0" />
         </HBox.margin>
      </TextField>
      <Button fx:id="pathButton" mnemonicParsing="false" onAction="#findPath" text="Audio File">
         <HBox.margin>
            <Insets left="1.0" top="1.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
